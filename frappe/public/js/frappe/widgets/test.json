	opp-table-gpu {
		compatible = "operating-points-v2";
		phandle = <0x31>;

		opp-120000000 {
			opp-hz = <0x00 0x7270e00>;
		};

		opp-312000000 {
			opp-hz = <0x00 0x1298be00>;
		};

		opp-432000000 {
			opp-hz = <0x00 0x19bfcc00>;
		};

		opp-576000000 {
			opp-hz = <0x00 0x22551000>;
		};
	};

	pmu {
		compatible = "arm,cortex-a7-pmu";
		interrupts = <0x00 0x78 0x04 0x00 0x79 0x04 0x00 0x7a 0x04 0x00 0x7b 0x04>;
		interrupt-affinity = <0x35 0x36 0x37 0x38>;
	};

	timer {
		compatible = "arm,armv7-timer";
		interrupts = <0x01 0x0d 0xf08 0x01 0x0e 0xf08 0x01 0x0b 0xf08 0x01 0x0a 0xf08>;
	};

	thermal-zones {

		cpu_thermal {
			polling-delay-passive = <0xfa>;
			polling-delay = <0x3e8>;
			thermal-sensors = <0x39>;

			trips {

				cpu_warm {
					temperature = <0x124f8>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3a>;
				};

				cpu_hot_pre {
					temperature = <0x13880>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3b>;
				};

				cpu_hot {
					temperature = <0x14c08>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3c>;
				};

				cpu_very_hot_pre {
					temperature = <0x15f90>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3d>;
				};

				cpu_very_hot {
					temperature = <0x17318>;
					hysteresis = <0x7d0>;
					type = "passive";
					phandle = <0x3e>;
				};

				cpu_crit {
					temperature = <0x19a28>;
					hysteresis = <0x7d0>;
					type = "critical";
					phandle = <0x7f>;
				};
			};

			cooling-maps {

				cpu_warm_limit_cpu {
					trip = <0x3a>;
					cooling-device = <0x35 0xffffffff 0x02>;
				};

				cpu_hot_pre_limit_cpu {
					trip = <0x3b>;
					cooling-device = <0x35 0x02 0x03>;
				};

				cpu_hot_limit_cpu {
					trip = <0x3c>;
					cooling-device = <0x35 0x03 0x04>;
				};

				cpu_very_hot_pre_limit_cpu {
					trip = <0x3d>;
					cooling-device = <0x35 0x05 0x06>;
				};

				cpu_very_hot_limit_cpu {
					trip = <0x3e>;
					cooling-device = <0x35 0x07 0xffffffff>;
				};
			};
		};
	};

